<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<artifactId>onprom-logextractor</artifactId>
	<name>Log extractor for OnProm</name>
	<description>A Prom plugin for extracting XES Log from database by utilizing ontologies</description>

    <parent>
        <groupId>it.unibz.inf.kaos</groupId>
        <artifactId>onprom</artifactId>
        <version>2.1.0</version>
        <relativePath>../pom.xml</relativePath>
    </parent>

    <properties>
        <openxes.version>2.26</openxes.version>
        <ontop.version>1.18.1-OnProm</ontop.version>
    </properties>

    <build>
        <plugins>
            <plugin>
                <groupId>org.apache.maven.plugins</groupId>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>${maven.compiler.version}</version>
                <configuration>
                    <source>${java.version}</source>
                    <target>${java.version}</target>
                </configuration>
            </plugin>
            <plugin>
                <groupId>org.codehaus.mojo</groupId>
                <artifactId>exec-maven-plugin</artifactId>
                <version>1.5.0</version>
                <configuration>
                    <includePluginDependencies>true</includePluginDependencies>
                    <mainClass>it.unibz.inf.kaos.logextractor.LogExtractor</mainClass>
                </configuration>
            </plugin>
        </plugins>
    </build>

    <!-- for Ontop 1.18.1-OnProm -->
    <repositories>
        <repository>
		    <id>thirdparty</id>
		    <url>http://obdavm.inf.unibz.it:8080/nexus/content/repositories/thirdparty</url>
            <releases>
                <enabled>true</enabled>
            </releases>
            <snapshots>
                <enabled>true</enabled>
            </snapshots>
        </repository>
    </repositories>
    <!-- END OF for Ontop 1.18.1-OnProm -->
	<dependencies>
	    <!-- =============================================================== -->
		<!-- Ontop -->
        <!-- =============================================================== -->
        <dependency>
            <groupId>it.unibz.inf.ontop</groupId>
            <artifactId>ontop-obdalib-core</artifactId>
            <version>${ontop.version}</version>
        </dependency>
		<dependency>
		    <groupId>it.unibz.inf.ontop</groupId>
    		<artifactId>ontop-quest-owlapi</artifactId>
            <version>${ontop.version}</version>
		</dependency>
		<!-- =============================================================== -->
		<!-- END OF Ontop -->
        <!-- =============================================================== -->
		
		<!-- =============================================================== -->
		<!-- JDBC Drivers -->
        <!-- =============================================================== -->
    	<dependency>
			<groupId>com.h2database</groupId>
			<artifactId>h2</artifactId>
            <version>1.4.197</version>
		</dependency>
    	<dependency>
    		<groupId>mysql</groupId>
    		<artifactId>mysql-connector-java</artifactId>
            <version>8.0.12</version>
    	</dependency>
		<dependency>
		    <groupId>org.postgresql</groupId>
		    <artifactId>postgresql</artifactId>
            <version>42.2.5</version>
		</dependency>
        <dependency>
            <groupId>com.ibm</groupId>
            <artifactId>db2jcc4.jar</artifactId>
            <version>10.1.4</version>
        </dependency>
        <dependency>
            <groupId>com.microsoft.sqlserver</groupId>
            <artifactId>mssql-jdbc</artifactId>
            <version>7.1.1.jre10-preview</version>
        </dependency>
        <dependency>
            <groupId>com.oracle.jdbc</groupId>
            <artifactId>ojdbc8</artifactId>
            <version>12.2.0.1</version>
        </dependency>
        <dependency>
            <groupId>net.sourceforge.jtds</groupId>
            <artifactId>jtds</artifactId>
            <version>1.3.1</version>
        </dependency>
        <!-- =============================================================== -->
		<!-- END OF JDBC Drivers -->
        <!-- =============================================================== -->
		
		<!-- =============================================================== -->
		<!-- Related to OnProm -->
        <!-- =============================================================== -->
        <dependency>
            <artifactId>onprom-data</artifactId>
            <groupId>${project.groupId}</groupId>
            <version>${project.version}</version>
        </dependency>
        
		<dependency>
            <artifactId>onprom-obdamapper</artifactId>
            <groupId>${project.groupId}</groupId>
            <version>${project.version}</version>
        </dependency>
        
		<!-- =============================================================== -->
		<!-- END OF Related to OnProm -->
        <!-- =============================================================== -->
        
        <!-- =============================================================== -->
		<!-- XES related libraries -->
        <!-- =============================================================== -->
		<dependency>
            <groupId>deckfour</groupId>
            <artifactId>spex</artifactId>
            <version>1.0</version>
        </dependency>
        <dependency>
            <groupId>deckfour</groupId>
            <artifactId>openxes</artifactId>
            <version>${openxes.version}</version>
        </dependency>
        <dependency>
            <groupId>io.github.andreas-solti.xeslite</groupId>
            <artifactId>xeslite</artifactId>
            <version>0.0.1</version>
            <exclusions>
                <exclusion>
                    <groupId>org.deckfour</groupId>
                    <artifactId>openxes</artifactId>
                </exclusion>
            </exclusions>
        </dependency>
        <dependency>
            <!-- needed for XLog to XML serialization in Java > 8 -->
            <groupId>javax.xml.bind</groupId>
            <artifactId>jaxb-api</artifactId>
            <version>2.4.0-b180830.0359</version>
        </dependency>
        <!-- =============================================================== -->
        <!-- END OF XES related libraries -->
        <!-- =============================================================== -->

        <!-- ################################################################ -->
	    <!-- Logger Related -->
		<!-- ################################################################ -->
    	<dependency>
    		<groupId>ch.qos.logback</groupId>
    		<artifactId>logback-core</artifactId>
    		<version>${logback.version}</version>
    	</dependency>
    	<!-- ################################################################ -->
	    <!-- END OF Logger Related -->
		<!-- ################################################################ -->



    	<!-- ################################################################ -->
	    <!-- Others -->
		<!-- ################################################################ -->

        <dependency>
            <groupId>com.fasterxml.jackson.core</groupId>
            <artifactId>jackson-core</artifactId>
            <version>${jackson.version}</version>
        </dependency>
        <dependency>
            <groupId>com.fasterxml.jackson.core</groupId>
            <artifactId>jackson-databind</artifactId>
            <version>${jackson.version}</version>
        </dependency>
        <dependency>
            <groupId>com.fasterxml.jackson.core</groupId>
            <artifactId>jackson-annotations</artifactId>
            <version>${jackson.version}</version>
        </dependency>

        <!-- ################################################################ -->
	    <!-- Others -->
		<!-- ################################################################ -->


      </dependencies>
</project>
